FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}

// Same geometry/mesh as `incompressible_backwards_step`.

scale 1;

vertices
(
    (0    0    0)
    (1    0    0)
    (3    0    0)

    (0    0.5  0)
    (1    0.5  0)
    (3    0.5  0)

    (0    1    0)
    (1    1    0)
    (3    1    0)

    (0    0    0.01)
    (1    0    0.01)
    (3    0    0.01)

    (0    0.5  0.01)
    (1    0.5  0.01)
    (3    0.5  0.01)

    (0    1    0.01)
    (1    1    0.01)
    (3    1    0.01)
);

blocks
(
    hex (3 4 7 6 12 13 16 15) (10 5 1) simpleGrading (1 1 1)
    hex (1 2 5 4 10 11 14 13) (20 5 1) simpleGrading (1 1 1)
    hex (4 5 8 7 13 14 17 16) (20 5 1) simpleGrading (1 1 1)
);

edges ();

boundary
(
    inlet
    {
        type patch;
        faces ((3 6 15 12));
    }
    outlet
    {
        type patch;
        faces
        (
            (2 11 14 5)
            (5 14 17 8)
        );
    }
    walls
    {
        type wall;
        faces
        (
            (6 7 16 15)
            (7 8 17 16)
            (1 2 11 10)
            (3 4 13 12)
            (1 10 13 4)
        );
    }
    frontAndBack
    {
        type empty;
        faces
        (
            (3 4 7 6)
            (12 15 16 13)

            (1 2 5 4)
            (10 13 14 11)

            (4 5 8 7)
            (13 16 17 14)
        );
    }
);

mergePatchPairs ();
